use puts(string);

main {
  for(1..5) {
    puts("hello world");
  };
  return 0;
}
------
declare i32 @puts(i8* noalias nocapture) nounwind
@str = private unnamed_addr constant [12 x i8] c"hello world\00"
define i32 @main() {
  br label %Entry
  Entry:
    br label %Loop
  Loop:
    %i = phi i32 [ 1, %Entry ], [ %nextvar, %Loop ]
  %var = getelementptr [12 x i8]* @str, i64 0, i64 0
  %var2 = call i32 (i8*)* @puts(i8* %var)
  %nextvar = add i32 %i, 1
  %break = icmp ugt i32 %nextvar, 5
  br i1 %break, label %Continue, label %Loop
  Continue:
  ret i32 0
}
